{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.31/schema.json",
  "app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 1,
  "default_container_padding": 1,
  "border": true,
  "border_width": 4,
  "border_offset": -1,
  "theme": {
    "palette": "Catppuccin",
    "name": "Frappe",
    "unfocused_border": "Rosewater",
    "bar_accent": "Mantle"
  },
  "stackbar": {
    "height": 40,
    "mode": "OnStack",
    "tabs": {
      "width": 300
    }
  },
  "monitors": [
    {
      "workspaces": [
          {
            "name": "AI",
            "layout": "BSP",
            "initial_workspace_rules":[
              {
              "id": "Noi.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
              }
            ]
          },
          {
            "name": "Browser",
            "layout": "BSP",
            "initial_workspace_rules":[
              {
              "id": "msedge",
              "kind": "Exe",
              "matching_strategy": "StartsWith"
              },
              {
                "id": "Arc.exe",
                "kind": "Exe",
                "matching_strategy": "Equals"
              }
            ]
          }
        ]
    },
    {
      "workspaces": [
          {
            "name": "office",
            "layout": "Rows",
            "initial_workspace_rules":[
              {
                "id": "olk.exe",
                "kind": "Exe",
                "matching_strategy": "Equals"
              },
              {
                "id": "ms-teams.exe",
                "kind": "Exe",
                "matching_strategy": "Equals"
              }
            ]
          },
          {
            "name": "time",
            "layout": "Rows",
            "initial_workspace_rules":[
              {
              "id": "Tick",
              "kind": "Exe",
              "matching_strategy": "StartsWith"
              }
            ]
          }
        ]
    },
    {
      "workspaces": [
        {
          "name": "terminal",
          "layout": "BSP",
          "initial_workspace_rules":[
              {
              "id": "wezterm.exe",
              "kind": "Exe",
              "matching_strategy": "StartsWith"
              }
          ]
        },
        {
          "name": "code",
          "layout": "BSP",
          "initial_workspace_rules":[
              {
              "id": "Code.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
              }
          ]
        },
        {
          "name": "notes",
          "layout": "BSP",
          "initial_workspace_rules":[
              {
              "id": "Heptabase.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
              },
              {
              "id": "Obsidian.exe",
              "kind": "Exe",
              "matching_strategy": "Equals"
              }
          ]
        }
      ]
    }
  ],
  "display_index_preferences": {
    "0": "AUO6092-4&1102d6c9&0&UID8388688",
    "1": "AUS24A1-4&1102d6c9&0&UID4145",
    "2": "DELA1A8-4&1102d6c9&0&UID8261",
    "3": "DEL422D-4&1102d6c9&0&UID8261",
    "4": "DELD141-4&1102d6c9&0&UID12613"
  }
}
